@model string[]
@{
    ViewBag.Title = "List";
    var newList = ViewBag.data as string[];
    var newList1 = ViewData["data"] as string[];

    var curpage = (int)ViewBag.Page;
    var start = curpage - 2;
}

<h2>List</h2>
<h3> 当前为第 @ViewBag.Page 页</h3>
<ol>
    @foreach (var item in newList)
    {
        <li>@item</li>
    }
</ol>
<ol>
    @foreach (var item in Model)
    {
        <li>@item</li>
    }
</ol>
<ol>
    @for (int i = 0; i < newList1.Length; i++)
    {
        <li>@newList1[i]</li>
    }
</ol>
<ol>
    @for (int i = 0; i < Model.Length; i++)
    {
        <li>@Model[i]</li>
    }
</ol>

<ol>
    @for (int i = start; i < curpage + 2; i++)
    {
        <li>
            @Html.ActionLink(string.Format("第{0}页", i), "List", "Home", new { page = i }, null)
        </li>
    }
</ol>